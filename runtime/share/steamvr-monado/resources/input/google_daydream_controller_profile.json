{
  "json_id": "input_profile",
  "controller_type": "monado_google_daydream_controller",
  "device_class": "TrackedDeviceClass_Controller",
  "resource_root": "steamvr-monado",
  "driver_name": "monado",
  "input_bindingui_mode": "controller_handed",
  "should_show_binding_errors": true,
  "input_bindingui_left": {
    "image": "{indexcontroller}/icons/indexcontroller_left.svg"
  },
  "input_bindingui_right": {
    "image": "{indexcontroller}/icons/indexcontroller_right.svg"
  },
  "input_source": {
    "/input/select": {
      "type": "button",
      "binding_image_point": [
        0,
        0
      ],
      "order": 6,
      "click": true,
      "touch": false
    },
    "/input/trackpad": {
      "type": "trackpad",
      "binding_image_point": [
        0,
        0
      ],
      "order": 9
    },
    "/pose/grip": {
      "type": "pose",
      "binding_image_point": [
        0,
        0
      ],
      "order": 10
    },
    "/pose/aim": {
      "type": "pose",
      "binding_image_point": [
        0,
        0
      ],
      "order": 11
    },
    "/pose/pinch_ext": {
      "type": "pose",
      "binding_image_point": [
        0,
        0
      ],
      "order": 14
    },
    "/pose/poke_ext": {
      "type": "pose",
      "binding_image_point": [
        0,
        0
      ],
      "order": 15
    },
    "/pose/palm_ext": {
      "type": "pose",
      "binding_image_point": [
        0,
        0
      ],
      "order": 17
    }
  }
}