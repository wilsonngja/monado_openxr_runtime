    "image": "{indexcontroller}/icons/indexcontroller_left.svg"
  },
  "input_bindingui_right": {
    "image": "{indexcontroller}/icons/indexcontroller_right.svg"
  },
  "input_source": {
    "/input/x": {
      "type": "button",
      "binding_image_point": [
