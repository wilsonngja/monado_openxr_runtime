{
  "json_id": "input_profile",
  "controller_type": "monado_microsoft_hand_interaction",
  "device_class": "TrackedDeviceClass_Controller",
  "resource_root": "steamvr-monado",
  "driver_name": "monado",
  "input_bindingui_mode": "controller_handed",
  "should_show_binding_errors": true,
  "input_bindingui_left": {
    "image": "{indexcontroller}/icons/indexcontroller_left.svg"
  },
  "input_bindingui_right": {
    "image": "{indexcontroller}/icons/indexcontroller_right.svg"
  },
  "input_source": {
    "/input/select": {
      "type": "trigger",
      "binding_image_point": [
        0,
        0
      ],
      "order": 4,
      "click": false,
      "touch": false,
      "value": true,
      "force": false
    },
    "/input/grip": {
      "type": "trigger",
      "binding_image_point": [
        0,
        0
      ],
      "order": 5,
      "click": false,
      "touch": false,
      "value": true,
      "force": false
    },
    "/pose/grip": {
      "type": "pose",
      "binding_image_point": [
        0,
        0
      ],
      "order": 6
    },
    "/pose/aim": {
      "type": "pose",
      "binding_image_point": [
        0,
        0
      ],
      "order": 7
    },
    "/pose/pinch_ext": {
      "type": "pose",
      "binding_image_point": [
        0,
        0
      ],
      "order": 10
    },
    "/pose/poke_ext": {
      "type": "pose",
      "binding_image_point": [
        0,
        0
      ],
      "order": 11
    },
    "/pose/palm_ext": {
      "type": "pose",
      "binding_image_point": [
        0,
        0
      ],
      "order": 13
    }
  }
}